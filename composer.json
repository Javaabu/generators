{
    "name": "javaabu/generators",
    "description": "Laravel generators on steroids",
    "homepage": "https://github.com/Javaabu/generators",
    "license": "MIT",
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0",
        "illuminate/testing": "^9.0 || ^10.0",
        "laracraft-tech/laravel-schema-rules": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^7.0 || ^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Generators\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Generators\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Generators\\GeneratorsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
